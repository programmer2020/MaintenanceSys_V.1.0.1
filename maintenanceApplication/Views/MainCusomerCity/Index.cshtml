@model IEnumerable<maintenanceApplication.Models.Maintenance.lookups.MaintenanceCustomerCityModel>

@{
    ViewBag.Title = "Index";
}
        <div class="col-md-1" ></div>
        <div class="col-md-9" style="margin:20px 0 0 10px;">
            <p>
                <a href="/MainCusomerCity/Create" class="btn btn-primary">Create New</a>
            </p>
            <table class="table">
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.CustomerCity)
                    </th>
                    <th></th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.CustomerCity)
                        </td>
                        <td>
                            @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                            @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                            @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                        </td>
                    </tr>
                }

            </table>
            <a href="/Maintenance/GetNewMaintenanceRequests" class="btn btn-warning" >Cancel</a>

        </div>
